<div class="text-center ">
  <div class="new_account container">
    <h1>ログイン</h1>
    <%= notice %>
    <%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>

      <%= f.label :email %>
      <%= f.email_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.submit "Log in" %>
    <% end %>
  </div>
</div>
